  "source": "pragma solidity ^0.4.19;\n\ncontract Homoglyph {\n  mapping (string => uint256) balances;\n\n  function () payable {\n    balances['result'] += msg.value;\n  }\n\n  // Cyrillic 'e' in result\n  function getBalance() constant returns (uint256) {\n    return balances['rÐµsult'];\n  }\n}",
